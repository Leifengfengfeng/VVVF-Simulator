<Window x:Class="VVVF_Simulator.GUI.UtilForm.Generate_Language_Select"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VVVF_Simulator.GUI.UtilForm"
        mc:Ignorable="d"
        Title="Generate_Language_Select" Height="221" Width="391" ResizeMode="NoResize">
    <Grid Margin="0,0,0,0">
        <Grid.RowDefinitions>
            <RowDefinition Height="5*"/>
            <RowDefinition Height="3*"/>
        </Grid.RowDefinitions>
        <Label Content="Select the language" HorizontalAlignment="Left" Margin="24,22,0,0" Grid.Row="0" VerticalAlignment="Top" FontFamily="M PLUS 2 Medium" FontWeight="Bold" FontSize="16" Height="30" Width="162"/>
        <ComboBox x:Name="string_lang_selector" HorizontalAlignment="Center" Grid.Row="0" VerticalAlignment="Top" Width="228" Margin="0,58,0,0" Height="22"/>
        <Button Content="OK" HorizontalAlignment="Center" Grid.Row="1" VerticalAlignment="Center" Width="167" Height="33" FontFamily="M PLUS 2" FontWeight="Bold" FontSize="16" Margin="0,10,0,10" Click="Button_Click"/>
    </Grid>
</Window>
